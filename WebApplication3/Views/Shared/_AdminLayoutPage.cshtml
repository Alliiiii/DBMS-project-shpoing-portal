<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width" />
    <title>AdminDashboard</title>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Sufee Admin - HTML5 Admin Template</title>
    <meta name="description" content="Sufee Admin - HTML5 Admin Template">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="apple-touch-icon" href="~/Content/templet1/apple-icon.png">
    <link rel="shortcut icon" href="~/Content/templet1/favicon.ico">
    <link rel="stylesheet" href="~/Content/templet1/~/Templete/assets/css/normalize.css">
    <link rel="stylesheet" href="~/Content/templet1/~/Templete/assets/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/Content/templet1/~/Templete/assets/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/Content/templet1/~/Templete/assets/css/themify-icons.css">
    <link rel="stylesheet" href="~/Content/templet1/~/Templete/assets/css/flag-icon.min.css">
    <link rel="stylesheet" href="~/Content/templet1/~/Templete/assets/css/cs-skin-elastic.css">
    <!-- <link rel="stylesheet" href="~/Content/templet1/~/Templete/assets/css/bootstrap-select.less"> -->
    <link rel="stylesheet" href="~/Content/templet1/~/Templete/assets/scss/style.css">
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,600,700,800' rel='stylesheet' type='text/css'>
    <!-- <script type="text/javascript" src="~/Content/templet1/https://cdn.jsdelivr.net/html5shiv/3.7.3/html5shiv.min.js"></script> -->


</head>
<body>


    <aside id="left-panel" class="left-panel">
        <nav class="navbar navbar-expand-sm navbar-default">
            <div class="navbar-header">
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#main-menu" aria-controls="main-menu" aria-expanded="false" aria-label="Toggle navigation">
                    <i class="fa fa-bars"></i>
                </button>
                <br /><br /><br />
            </div>
            <div id="main-menu" class="main-menu collapse navbar-collapse">
                <ul class="nav navbar-nav">
                    <li class="active">
                        <a href="~/Content/templet1/@Url.Action("AdminDashboard")"> <i class="menu-icon fa fa-dashboard"></i>Admin Dashboard </a>
                    </li>
                    <h3 class="menu-title"></h3><!-- /.menu-title -->
                    <li class="menu-item-has-children dropdown">
                        <a href="~/Content/templet1/#" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"> <i class="menu-icon fa fa-laptop"></i>Package Details</a>
                        <ul class="sub-menu children dropdown-menu">
                            <li><i class="fa fa-puzzle-piece"></i><a href="~/Content/templet1/@Url.Action("PkgList")">Package List</a></li>
                            <li><i class="fa fa-id-badge"></i><a href="~/Content/templet1/@Url.Action("AddPkg")">Add Packages</a></li>
                        </ul>
                    </li>
                    <li class="menu-item-has-children dropdown">
                        <a href="~/Content/templet1/#" class="dropdown-toggle" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"> <i class="menu-icon fa fa-table"></i>Services Details</a>
                        <ul class="sub-menu children dropdown-menu">
                            <li><i class="fa fa-table"></i><a href="~/Content/templet1/@Url.Action("SerList")">Services List</a></li>
                            <li><i class="fa fa-table"></i><a href="~/Content/templet1/@Url.Action("Services")">Add Services</a></li>
                        </ul>
                    </li>
                </ul>
            </div><!-- /.navbar-collapse -->
        </nav>
    </aside>
    <div id="right-panel" class="right-panel">

        <div class="content mt-3">
            <div class="animated fadeIn">

                <div class="row" style="margin-left:200px;">

                    <script src="~/Content/templet1/~/Scripts/modernizr-2.6.2.js"></script>



                    <div>
                        @RenderBody()
                    </div>


                </div>
            </div>
        </div>
    </div>
    <script src="~/Content/templet1/~/Templete/assets/js/vendor/jquery-2.1.4.min.js"></script>
    <script src="~/Content/templet1/~/Templete/assets/js/popper.min.js"></script>
    <script src="~/Content/templet1/~/Templete/assets/js/plugins.js"></script>
    <script src="~/Content/templet1/~/Templete/assets/js/main.js"></script>
</body>
</html>
